{
  "name": "lwc-web-app",
  "version": "1.0.0",
  "description": "Here will be some information about the app.",
  "main": "server-api.js",
  "scripts": {
    "build": "lwc-services build -m production -w webpack.config.js",
    "build:development": "lwc-services build -w scripts/webpack.config.js",
    "lint": "eslint ./src/**/*.ts",
    "prettier": "prettier --write \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
    "prettier:verify": "prettier --list-different \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
    "serve": "node scripts/server.js",
    "test:unit": "lwc-services test:unit",
    "test:unit:coverage": "lwc-services test:unit --coverage",
    "test:unit:debug": "lwc-services test:unit --debug",
    "test:unit:watch": "lwc-services test:unit --watch",
    "watch": "lwc-services watch -w scripts/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VenkatKoppolu/lwc-web-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VenkatKoppolu/lwc-web-app/issues"
  },
  "homepage": "https://github.com/VenkatKoppolu/lwc-web-app#readme",
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/helmet": "^0.0.48",
    "@types/node": "^14.11.2",
    "@types/validator": "^13.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "sequelize": "^6.3.5",
    "ts-loader": "^8.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^2.5.2",
    "pg": "^8.4.0"
  },
  "dependencies": {}
}
